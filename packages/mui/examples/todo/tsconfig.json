{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "module": "commonjs",
    "target": "esnext",
    "lib": [
      "es6",
      "dom",
      "esnext.asynciterable"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}